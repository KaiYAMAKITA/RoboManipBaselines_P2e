<mujoco model="ur5e_cable_verticalup">
  <compiler angle="radian" meshdir="../robots/meshes" texturedir="../robots/textures" autolimits="true"/>
  <option timestep="0.004" integrator="implicitfast"/>
  <size memory="2M"/>

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.1 0.1 0.1" specular="0.0 0.0 0.0"/>
    <rgba haze="1.0 1.0 1.0 1.0"/>
    <global azimuth="120" elevation="-20"/>
  </visual>

  <include file="../robots/ur5e/ur5e_config.xml"/>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.9 1.0 1.0" rgb2="0.9 1.0 1.0" width="512" height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" rgb1="0.6 0.6 0.6" rgb2="0.7 0.7 0.7" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5"/>
    <texture name="marble1" file="table/marble1.png" type="2d"/>
    <material name="marble1" texture="marble1" specular="0.0" shininess="0.0"/>
    <material name="table_base" rgba="1.0 0.93 0.7 1"/>
    <material name="stand" rgba="0.05 0.22 0.48 1"/>
  </asset>

  <worldbody>
    <light pos="0 0 1.5" dir="0 0 -1" directional="true" diffuse="0.5 0.5 0.5"/>

    <camera name="front" mode="fixed" pos="1.0 0.0 2.0" euler="0.0 0.785398 1.5708"/>
    <camera name="side" mode="fixed" pos="0.0 -1.25 1.0" euler="1.5708 0.0 0.0"/>

    <geom name="floor" size="0 0 0.05" type="plane" material="groundplane"/>

    <body name="table">
      <geom type="box" size="0.275 0.5 0.4" pos="0 0 0.4" material="table_base"/>
      <geom type="box" size="0.275 0.5 0.0075" pos="0 0 0.8075" material="marble1"/>
    </body>

    <body name="stand">
      <geom type="box" size="0.33 0.33 0.4" pos="-0.605 0 0.4" material="stand"/>
    </body>

    <body name="ur5e_root_frame" pos="-0.605 0.0 0.8">
      <include file="../robots/ur5e/ur5e_body.xml"/>
    </body>

    <body name="B0" pos="-0.175 0.4 0.8325" euler="0 0 -1.5708">
      <geom type="box" size="0.02 0.015 0.015" pos="0.0 0.0 0.0" rgba="0.1 0.8 0.1 1.0"/>
      <!-- <freejoint/> -->
      <composite type="rope" count="40" spacing="0.02">
        <joint kind="main" damping="0.005"/>
        <geom type="capsule" size="0.01 0.0075" rgba="0.8 0.8 0.2 1"/>
      </composite>
    </body>

    <body name="cable_end" pos="-0.175 -0.4 0.8325" euler="0 0 -1.5708">
      <freejoint/>
      <geom type="box" size="0.02 0.015 0.015" pos="0.0 0.0 0.0" rgba="0.1 0.8 0.1 1.0"/>
    </body>

    <body name="poles" pos="-0.1 0.2 0.815">
      <geom type="cylinder" size="0.02 0.05" pos="0.0 0.0 0.05" rgba="0.8 0.5 0.1 1.0"/>
      <geom type="cylinder" size="0.02 0.05" pos="0.1 0.0 0.05" rgba="0.8 0.5 0.1 1.0"/>
    </body>
  </worldbody>

  <equality>
    <weld name="right_boundary" body1="B39" body2="cable_end" relpose="0 0 0 1 0 0 0"/>
  </equality>
</mujoco>
